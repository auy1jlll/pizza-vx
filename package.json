{
  "name": "resto-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:safe": "node workspace-protector.js start && npm run backup:auto && next dev --port 3007",
    "dev:original": "next dev --port 3007",
    "dev:fresh": "npm run clear-cache && npm run dev:safe",
    "start:local": "next start --port 3007",
    "dev:stop": "node workspace-protector.js stop",
    "clear-cache": "pwsh -ExecutionPolicy Bypass -File ./clear-cache.ps1",
    "clear-cache:manual": "rimraf .next && rimraf node_modules/.cache && rimraf tsconfig.tsbuildinfo",
    "build": "next build",
    "start": "next start --port 3005",
    "lint": "next lint",
    "backup": "node backup-data.js",
    "backup:auto": "node data-manager.js auto",
    "restore": "node smart-restore.js",
    "restore:force": "node smart-restore.js --force",
    "restore:unsafe": "node data-manager.js restore",
    "data:list": "node data-manager.js list",
    "data:menu": "node data-manager.js",
    "workspace:status": "node workspace-protector.js status",
    "workspace:protect": "node workspace-protector.js start",
    "workspace:unprotect": "node workspace-protector.js stop",
    "db:backup-and-push": "npm run backup && npx prisma db push",
    "db:backup-and-generate": "npm run backup && npx prisma generate",
    "git:safe-commit": "npm run backup && git add . && git commit -m",
    "emergency:restore": "node workspace-protector.js protect && node smart-restore.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@types/lodash": "^4.17.20",
    "@types/lru-cache": "^7.10.9",
    "@types/nodemailer": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.3",
    "postcss": "^8.4.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "18.3.12",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "playwright": "^1.55.0",
    "prisma": "^6.15.0",
    "react-devtools": "^6.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
